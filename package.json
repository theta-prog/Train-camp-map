{
  "name": "campsite-search-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --project tsconfig.strict.json",
    "type-check": "tsc --noEmit",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:changed": "jest --changedSince=HEAD~1 --coverage",
    "test:staged": "jest --findRelatedTests $(git diff --cached --name-only --diff-filter=ACM | grep -E '\\.(ts|tsx|js|jsx)$' | tr '\\n' ' ') --coverage",
    "test:uncommitted": "jest --findRelatedTests $(git diff --name-only --diff-filter=ACM | grep -E '\\.(ts|tsx|js|jsx)$' | tr '\\n' ' ') --coverage",
    "test:related": "jest --findRelatedTests",
    "test:file": "jest --coverage --testPathPattern",
    "test:smart": "node scripts/smart-test.js",
    "test:smart:watch": "node scripts/smart-test.js uncommitted --watch",
    "test:smart:staged": "node scripts/smart-test.js staged",
    "test:smart:focused": "node scripts/smart-test.js uncommitted --focused",
    "test:smart:focused:watch": "node scripts/smart-test.js uncommitted --focused --watch",
    "test:smart:focused:staged": "node scripts/smart-test.js staged --focused",
    "test:types": "tsc --noEmit --skipLibCheck false",
    "validate": "npm run typecheck && npm run lint",
    "validate:prod": "npm run build && npm run lint",
    "validate:strict": "npm run typecheck:strict && npm run lint",
    "full-check": "./scripts/typecheck.sh",
    "ci": "npm run typecheck && npm run lint && npm run test:coverage && npm run build",
    "admin:create": "node scripts/create-admin.js",
    "admin:help": "echo 'Usage: ADMIN_PASSWORD=your_password npm run admin:create'",
    "setup:prod": "./scripts/setup-production.sh",
    "db:migrate": "npx prisma migrate deploy",
    "db:schema": "psql $DATABASE_URL -f database/postgresql-schema.sql",
    "jwt:generate": "node scripts/generate-jwt-secret.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.12.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vis.gl/react-google-maps": "^1.5.3",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.1",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.541.0",
    "next": "^14.2.30",
    "next-auth": "^4.24.11",
    "next-intl": "^4.1.0",
    "prisma": "^6.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.58.1",
    "typescript": "^5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie": "^0.6.0",
    "@types/google.maps": "^3.54.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
